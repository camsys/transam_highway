.modal-dialog.modal-lg
  .modal-content
    .modal-header
      %button.close{:data => {:dismiss => "modal"}}
        x
      %h3.modal-title
        = @child_element.new_record? ? 'Add Protective System' : 'Edit Protective System'
    .modal-body
      = render(:partial => 'shared/form_errors', :locals => {:obj => @child_element}) unless @child_element.errors.empty?

      = render "child_elements/form"