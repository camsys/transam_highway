#locationFilterContainer.panel-group{role:"tablist", "aria-multiselectable":"true"}
  .panel.panel-default
    .panel-heading{role:"tab"}
      %h5.panel-title
        Location
    .panel-collapse.collapse{role:"tabpanel", id:"location_filters"}
      .panel-body
        = f.collection_check_boxes :regions, Region.active.order(:name).pluck(:code, :name), :first, :last