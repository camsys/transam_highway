<p id="notice"><%= notice %></p>

<h1>Streambed Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @streambed_profiles.each do |streambed_profile| %>
      <tr>
        <td><%= link_to 'Show', streambed_profile %></td>
        <td><%= link_to 'Edit', edit_streambed_profile_path(streambed_profile) %></td>
        <td><%= link_to 'Destroy', streambed_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Streambed Profile', new_streambed_profile_path %>
